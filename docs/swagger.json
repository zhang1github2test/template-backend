{
    "swagger": "2.0",
    "info": {
        "description": "template-backend管理系统的API文档",
        "title": "template-backend API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/api",
    "paths": {
        "/api/plans": {
            "get": {
                "description": "分页获取招生计划信息，支持条件筛选",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "招生计划"
                ],
                "summary": "获取招生计划列表",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "页码，默认1",
                        "name": "page",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "每页数量，默认10",
                        "name": "page_size",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "学校名称",
                        "name": "school_name",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "年份",
                        "name": "year",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "区属类型",
                        "name": "district_type",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.HighSchoolAdmissionPlanPageResponseDoc"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/dto.HighSchoolAdmissionPlanPageResponseDoc"
                        }
                    }
                }
            },
            "post": {
                "description": "新增一条招生计划记录",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "招生计划"
                ],
                "summary": "创建招生计划",
                "parameters": [
                    {
                        "description": "招生计划对象",
                        "name": "plan",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/model.HighSchoolAdmissionPlan"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.HighSchoolAdmissionPlanResponseDoc"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/dto.HighSchoolAdmissionPlanResponseDoc"
                        }
                    }
                }
            }
        },
        "/api/plans/{id}": {
            "get": {
                "description": "根据ID获取招生计划详情",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "招生计划"
                ],
                "summary": "获取单个招生计划详情",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "招生计划ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.HighSchoolAdmissionPlanResponseDoc"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/dto.HighSchoolAdmissionPlanResponseDoc"
                        }
                    }
                }
            },
            "put": {
                "description": "根据ID更新招生计划信息",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "招生计划"
                ],
                "summary": "更新招生计划",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "招生计划ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "description": "更新内容",
                        "name": "plan",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/model.HighSchoolAdmissionPlan"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.HighSchoolAdmissionPlanResponseDoc"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/dto.HighSchoolAdmissionPlanResponseDoc"
                        }
                    }
                }
            },
            "delete": {
                "description": "根据ID删除招生计划",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "招生计划"
                ],
                "summary": "删除招生计划",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "招生计划ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.HighSchoolAdmissionPlanResponseDoc"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/dto.HighSchoolAdmissionPlanResponseDoc"
                        }
                    }
                }
            }
        }
